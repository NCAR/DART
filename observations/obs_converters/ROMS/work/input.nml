                       "gridded Aquarius SSS"             , "SATELLITE_SSS",
                       "glider MBARI"                     , "GLIDER_SALINITY",
                       "glider temperature NSF OOI"       , "GLIDER_TEMPERATURE",
                       "NOAA tide station sea level"      , "SEA_SURFACE_HEIGHT",

