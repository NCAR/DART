   type_translations = "gridded AVISO sea level anomaly"  , "SATELLITE_SSH",
                       "XBT from Met Office"              , "XBT_TEMPERATURE",
                       "glider UCSD "                     , "GLIDER_U_CURRENT_COMPONENT",
